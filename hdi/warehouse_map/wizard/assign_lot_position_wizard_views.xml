<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_assign_lot_position_wizard_form" model="ir.ui.view">
        <field name="name">assign.lot.position.wizard.form</field>
        <field name="model">assign.lot.position.wizard</field>
        <field name="arch" type="xml">
            <form string="Gán vị trí cho Lot/Pallet">
                <group>
                    <field name="warehouse_map_id" invisible="1"/>
                    <field name="posx" invisible="1"/>
                    <field name="posy" invisible="1"/>
                    <field name="posz" readonly="1" force_save="1"/>
                    <field name="lot_id" widget="lot_selector"/>
                    <field name="lot_name" invisible="1"/>
        </field>
    </record>
</odoo>
