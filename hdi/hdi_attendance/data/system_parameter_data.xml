<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_config_parameter_max_attendance_request" model="ir.config_parameter">
            <field name="key">en_max_attendance_request_count</field>
            <field name="value">3</field>
        </record>

        <record id="ir_config_parameter_attendance_request_start_day" model="ir.config_parameter">
            <field name="key">en_attendance_request_start</field>
            <field name="value">25</field>
