<odoo>
    <!-- Client Action for Dashboard -->
    <record id="action_warehouse_3d_dashboard" model="ir.actions.client">
        <field name="name">Warehouse 3D Dashboard</field>
        <field name="tag">warehouse_3d_dashboard</field>
        <field name="target">current</field>
    </record>

    <!-- Menu item for Dashboard -->
    <menuitem id="menu_warehouse_3d_dashboard"
              name="Dashboard"
              parent="menu_warehouse_3d_root"
              action="action_warehouse_3d_dashboard"
              sequence="1"/>

    <!-- Client Action for Fullscreen Viewer -->
    <record id="action_warehouse_3d_fullscreen" model="ir.actions.client">
        <field name="name">Warehouse 3D Viewer</field>
        <field name="tag">warehouse_3d_fullscreen</field>
        <field name="target">fullscreen</field>
    </record>
</odoo>
