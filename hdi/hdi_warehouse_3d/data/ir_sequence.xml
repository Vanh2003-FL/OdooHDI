<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequences -->
        <record id="seq_warehouse_crossdock" model="ir.sequence">
            <field name="name">Cross-Dock Sequence</field>
            <field name="code">warehouse.crossdock</field>
            <field name="prefix">XD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_warehouse_pick_tote" model="ir.sequence">
            <field name="name">Pick-to-Tote Sequence</field>
            <field name="code">warehouse.pick.tote</field>
            <field name="prefix">TOTE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
