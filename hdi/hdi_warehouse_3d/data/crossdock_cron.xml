<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Cron: Check Overdue Cross-Docks -->
    <record id="ir_cron_check_overdue_crossdock" model="ir.cron">
        <field name="name">Check Overdue Cross-Dock Operations</field>
        <field name="model_id" ref="model_warehouse_crossdock"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_overdue()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
    </record>

</odoo>
